<!DOCTYPE node PUBLIC "-//freedesktop//DTD D-BUS Object Introspection 1.0//EN"
                      "http://www.freedesktop.org/standards/dbus/1.0/introspect.dtd">
<node>
  <interface name="com.jappeos.Core.SessionManagerService">
    <method name="CreateSession">
      <arg name="username" type="s" direction="in"/>
      <arg name="password" type="s" direction="in"/>
      <arg name="sessionId" type="s" direction="out"/>
      <arg name="uid" type="u" direction="out"/>
      <arg name="seat" type="s" direction="out"/>
    </method>

    <method name="StopSession">
      <arg name="sessionId" type="s" direction="in"/>
    </method>

    <method name="ListSessions">
      <arg name="sessions" type="a(ssuss)" direction="out"/>
    </method>
  </interface>
</node>
